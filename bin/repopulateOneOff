Firebaseref()
    .pipe(SetContentTypeData(contentType, dataToRepopulate))
    .pipe(Exit());

function SetContentTypeData (contentType, data) {
    return through.obj(setter);

    function setter (fb, enc, next) {
        var stream = this;

        fb.child('data')
            .child(contentType)
            .set(data, function () {
                stream.push({});
                next();
            });
    }
}

function Exit () {
    return through.obj(exit);

    function exit (row, enc, next) {
        console.log('Done!');
        this.push(null);
        process.exit();
    }
}
